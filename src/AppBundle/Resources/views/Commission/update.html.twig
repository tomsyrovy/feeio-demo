{% extends "::layout-secured-simple.html.twig" %}
{% set title = "Úprava zakázky" %}
{% block title %}{{ title }} {{ titleSeparator }} {{ parent() }}{% endblock %}
{% block body_class %}page-commission-create page-commission-update {{ parent() }}{% endblock %}

{% block content %}

    <div class="row">

        <div class="col-lg-12">
            <a class="btn btn-info" title="Zpět" href="{{ path('app_commission_detail', {'commission_id' : commission.id}) }}"><i class="fa fa-fw fa-caret-left"></i> Zpět</a>

            <div class="page-header text-center">
                <h2><i class="fa fa-fw fa-money"></i> {{ title }}</h2>
            </div>
        </div> <!-- .col-lg-12 -->


        {% include "@App/Commission/inc/commissionForm.html.twig" with {"form" : form, "submitLabel" : "Uložit"} %}


    </div> <!-- .row -->


{% endblock %}

{% block javascripts %}

    {{ parent() }}

    {% include '@App/Commission/inc/commissionFormJS.html.twig' %}

{% endblock %}
